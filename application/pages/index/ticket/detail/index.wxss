.container {
  height: 100%;
  width: 100%;
  background: #f5f5f5;
  color: #333;
  font-size: 28rpx;
  overflow-x: hidden;
  overflow-y: scroll;
}

.header {
  position: relative;
  height: 128rpx;
  padding: 56rpx 0 0;
  background: #fff;
  color: #000;
  font-size: 37rpx;
  text-align: center;
}

.header.android {
  height: 150rpx;
  padding: 74rpx 0 0;
}

.icon-back {
  position: absolute;
  left: 20rpx;
  bottom: 25rpx;
  width: 35rpx;
  height: 35rpx;
}

.main {
  height: calc(100% - 128rpx);
  width: 100%;
  background: #f5f5f5;
  overflow-x: hidden;
  overflow-y: scroll;
}

.main.android {
  height: calc(100% - 150rpx);
}

.detail-top {
  position: relative;
  height: 392rpx;
  width: 100%;
  padding: 40rpx 30rpx 0 30rpx;
  overflow: hidden;
}

.top-bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  -webkit-filter: blur(35px);
  -moz-filter: blur(35px);
  -o-filter: blur(35px);
  -ms-filter: blur(35px);
  filter: blur(35px);
  z-index: 0;
}

.top-left {
  position: relative;
  float: left;
  width: 240rpx;
  height: 306rpx;
  border-radius: 10rpx;
}

.top-right {
  position: relative;
  width: 430rpx;
  float: right;
}

.t-r-title {
  font-size: 34rpx;
  color: #fff;
  font-weight: bold;
}

.t-r-price {
  position: absolute;
  font-size: 32rpx;
  color: #fff;
  top: 270rpx;
  font-weight: bold;
}

.ticket-pic {
  width: 240rpx;
  height: 306rpx;
  border-radius: 6rpx;
}

.content {
  width: 100%;
  background: #fff;
}

.c-top {
  position: relative;
  height: auto;
  width: 100%;
  margin-left: 30rpx;
  padding-top: 36rpx;
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #f5f5f5;
}

.c-top:nth-child(2) {
  border-bottom: none;
}

.c-t-time {
  font-size: 34rpx;
  font-weight: 400;
  width: 587rpx;
  color: rgba(26, 26, 26, 1);
  line-height: 48rpx;
}

.c-t-timelong {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(155, 155, 155, 1);
  line-height: 42rpx;
  width: 587rpx;
}

.c-t-setpic {
  position: absolute;
  width: 100rpx;
  top: 44rpx;
  right: 32rpx;
}

.setpic {
  width: 32rpx;
  height: 30rpx;
  margin-left: 18rpx;
}

.set-text {
  color: #f60;
  font-size: 22rpx;
}

.mappic {
  width: 52rpx;
  height: 52rpx;
  margin-left: 11rpx;
}

.c-buttom {
  height: 160rpx;
  width: 100%;
  padding-left: 30rpx;
}

.bot {
  position: fixed;
  height: 172rpx;
  background: #fff;
  width: 100%;
  bottom: 0;
  padding: 36rpx 30rpx 36rpx 30rpx;
}

.btn {
  width: 690rpx;
  height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  line-height: 100rpx;
  background: rgba(255, 153, 0, 1);
  border-radius: 10rpx;
}

.info {
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  background: #fff;
  min-height: 400rpx;
  margin-bottom: 172rpx;
}

.info-tab {
  height: 108rpx;
  width: 100%;
  border-bottom: 1rpx solid #f5f5f5;
}

.tab {
  position: relative;
  height: 108rpx;
  line-height: 108rpx;
  text-align: center;
  font-size: 32rpx;
  color: #999;
  float: left;
}

.tab:nth-child(1) {
  margin-left: 198rpx;
}

.tab:nth-child(2) {
  margin-left: 226rpx;
}

.tab-bottom {
  position: absolute;
  bottom: 0;
  width: 40rpx;
  height: 6rpx;
  left: 12rpx;
  background: rgba(255, 102, 0, 1);
  border-radius: 4rpx;
}

.active {
  color: #000;
}

.info-content {
  padding: 28rpx 36rpx 28rpx 36rpx;
}

.content-title {
  margin-bottom: 26rpx;
  font-size: 32rpx;
  color: #333;
}

.article {
  font-size: 28rpx;
  line-height: 50rpx;
  color: #666;
}

.popupBg {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 998;
}

.buy {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 956rpx;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  background: #fff;
  animation: buyPopup 0.2s;
  animation-fill-mode: forwards; /*当动画完成时，动画会停留在最后一帧。*/
  margin-bottom: 100rpx;
  z-index: 999;
}

.buy-content {
  position: relative;
  width: 100%;
  height: 100%;
  padding: 30rpx;
  overflow-y: scroll;
  margin-bottom: 100rpx;
}

.close {
  position: absolute;
  top: 40rpx;
  right: 30rpx;
  width: 28rpx;
  height: 28rpx;
}

.session {
  margin-top: 50rpx;
  overflow: auto;
}

.session-title {
  font-size: 34rpx;
  color: #1a1a1a;
  font-weight: bold;
}

.session-list {
  width: 320rpx;
  height: 96rpx;
  text-align: center;
  line-height: 96rpx;
  background: #f5f5f5;
  border-radius: 6rpx;
  margin: 20rpx 14rpx 0 0;
  color: #1a1a1a;
  float: left;
}

.session-choose {
  width: 320rpx;
  height: 96rpx;
  text-align: center;
  line-height: 96rpx;
  background: rgba(255, 102, 0, 0.12);
  border-radius: 6rpx;
  margin: 20rpx 14rpx 0 0;
  color: #f60;
  float: left;
}

.ticket {
  margin-top: 50rpx;
  overflow: auto;
}

.ticket-title {
  font-size: 34rpx;
  color: #1a1a1a;
  font-weight: bold;
}

.ticket-list {
  width: 217rpx;
  line-height: 45rpx;
  padding: 24rpx;
  background: #f5f5f5;
  border-radius: 6rpx;
  margin: 20rpx 14rpx 0 0;
  color: #1a1a1a;
  float: left;
}

.ticket-choose {
  width: 217rpx;
  line-height: 45rpx;
  padding: 24rpx;
  background: rgba(255, 102, 0, 0.12);
  border-radius: 6rpx;
  margin: 20rpx 14rpx 0 0;
  color: #f60;
  float: left;
}

.number {
  position: relative;
  margin-top: 50rpx;
  overflow: auto;
  width: 100%;
}

.number-title {
  font-size: 34rpx;
  color: #1a1a1a;
  font-weight: bold;
  width: 30%;
  float: left;
}

.number-btn {
  position: absolute;
  right: 0;
  top: 0;
  width: 240rpx;
}

.num-add {
  width: 36rpx;
  height: 36rpx;
  float: left;
}

.num {
  width: 132rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
  float: left;
  text-align: center;
  line-height: 34rpx;
}

.num-minus {
  width: 36rpx;
  height: 36rpx;
}

.agreement {
  position: relative;
  margin-top: 70rpx;
  overflow: auto;
  width: 100%;
  font-size: 24rpx;
}

.agree {
  width: 24rpx;
  height: 24rpx;
  margin-right: 20rpx;
}

.agree-text {
  color: #f60;
}

.buy-ticket {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  box-shadow: 0rpx -4rpx 8rpx 0rpx rgba(0, 0, 0, 0.06);
  background: #fff;
}

.money {
  width: 400rpx;
  height: 100%;
  text-indent: 30rpx;
  font-size: 44rpx;
  font-weight: bold;
  line-height: 100rpx;
  float: left;
}

.doIn {
  width: 250rpx;
  background: #ccc;
  color: #fff;
  font-size: 36rpx;
  text-align: center;
  line-height: 100rpx;
  float: right;
}

.okIn {
  width: 250rpx;
  background: #f60;
  color: #fff;
  font-size: 36rpx;
  text-align: center;
  line-height: 100rpx;
  float: right;
}

@keyframes buyPopup {
  0% {
    bottom: -956rpx;
  }

  100% {
    bottom: 0;
  }
}

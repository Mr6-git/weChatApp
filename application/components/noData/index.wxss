.noList-wrap {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate3d(-50%, -50%, 0);
}
.noList-wrap, .noData-wrap {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}
.noList-img {
  width: 280rpx;
  height: 280rpx;
}
.noList-text {
  color: #9B9B9B;
  font-size: 26rpx;
}